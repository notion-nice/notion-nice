{
  "name": "nextjs-notion-starter-kit",
  "version": "2.0.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "next dev -p 4242",
    "build": "next build",
    "start": "next start",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "deploy": "next build && pm2 startOrRestart ecosystem.config.js",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test:stripe": "stripe listen --skip-verify --forward-to localhost:4242/api/stripe/webhook",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/language": "^6.10.1",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.2",
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@keyvhq/core": "^1.6.9",
    "@keyvhq/redis": "^1.6.10",
    "@lezer/highlight": "^1.2.0",
    "@next-auth/prisma-adapter": "0.4.4-canary.81",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@stripe/stripe-js": "^4.1.0",
    "@tryfabric/martian": "^1.2.4",
    "@vercel/og": "^0.0.19",
    "@vercel/postgres": "^0.7.2",
    "body-parser": "^1.20.2",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.1",
    "clsx": "^2.1.1",
    "codemirror": "^6.0.1",
    "cos-js-sdk-v5": "^1.7.0",
    "cos-nodejs-sdk-v5": "^2.13.4",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.10",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.4.1",
    "fflate": "^0.8.2",
    "got": "^12.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "lodash-es": "^4.17.21",
    "lqip-modern": "^2.0.0",
    "mysql2": "^3.11.0",
    "next": "12",
    "next-auth": "4.24.7",
    "notion-client": "^6.15.6",
    "notion-types": "^6.15.6",
    "notion-utils": "^6.15.6",
    "p-map": "^5.3.0",
    "p-memoize": "^6.0.1",
    "posthog-js": "^1.20.2",
    "react": "^18.2.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "^18.2.0",
    "react-icons": "4.8.1-snapshot.1",
    "react-notion-x": "^6.15.6",
    "react-tweet-embed": "^2.0.0",
    "react-use": "^17.4.2",
    "rss": "^1.2.2",
    "sharp": "0.32.6",
    "stripe": "^14.20.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.1",
    "@trivago/prettier-plugin-sort-imports": "^3.3.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^2.7.1",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^4.8.4"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}
